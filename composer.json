{
    "name": "navi-ai/php-sdk",
    "version": "1.4.2",
    "description": "Official PHP SDK for Navi AI Agent Platform - Build AI-powered conversations with ease",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "navi",
        "ai",
        "agent",
        "chatbot",
        "llm",
        "conversational-ai",
        "sdk",
        "api-client"
    ],
    "homepage": "https://github.com/deivid11/navi-ai-sdk",
    "authors": [
        {
            "name": "Navi Team"
        }
    ],
    "support": {
        "issues": "https://github.com/deivid11/navi-ai-sdk/issues",
        "source": "https://github.com/deivid11/navi-ai-sdk"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Navi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Navi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
